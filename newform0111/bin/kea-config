#!/usr/bin/env bash

usage()
{
    cat <<EOF
Usage: kea-config [OPTIONS]
Options:
     [--prefix]
     [--version]
     [--libs]
     [--cflags]
     [--ldflags]
     [--includes]
     [--hdfincludes]
     [--hdfcflags]
     [--hdflibs]
EOF
    exit $1
}
if test $# -eq 0; then
  usage 1 1>&2
fi
while test $# -gt 0; do
case "$1" in
    -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) optarg= ;;
esac
case $1 in
    --prefix)
    echo -n "/Users/plewis/Documents/GitHub/newform0111/newform0111 "
     ;;
	--version)
    echo -n "1.4.13 "
     ;;
    --cflags)
    echo -n "-I/Users/plewis/Documents/GitHub/newform0111/newform0111/include "
      ;;
    --libs)
      echo -n "-L/Users/plewis/Documents/GitHub/newform0111/newform0111/lib -lkea "
      ;;
    --ldflags)
      echo -n "/Users/plewis/Documents/GitHub/newform0111/newform0111/lib "
      ;;
    --includes)
      echo -n "/Users/plewis/Documents/GitHub/newform0111/newform0111/include "
      ;;
	--hdfincludes)
      echo -n " /Users/plewis/Documents/GitHub/newform0111/newform0111/include "
      ;;
	--hdfcflags)
      echo -n " -I/Users/plewis/Documents/GitHub/newform0111/newform0111/include "
      ;;
	--hdflibs)
      echo -n " -L/Users/plewis/Documents/GitHub/newform0111/newform0111/lib -L/usr/lib -lhdf5_cpp -lhdf5 -lpthread -lz -ldl -lm "
      ;;
  esac
  shift
done
echo
